<Window x:Class="SEP_Age.Auto.RegUser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SEP_Age.Auto"
        mc:Ignorable="d"
        Title="RegUser" Height="356" Width="300">
    <StackPanel>
        <StackPanel Margin="15" Orientation="Horizontal">
            <TextBox Name="BoxName" MinWidth="120"/>
            <Label Content="Имя"/>
        </StackPanel>
        <StackPanel Margin="15" Orientation="Horizontal">
            <TextBox Name="BoxEmail" MinWidth="120"/>
            <Label Content="Email"/>
        </StackPanel>
        <StackPanel Margin="15" Orientation="Horizontal">
            <TextBox Name="BoxPass" MinWidth="120"/>
            <Label Content="Пароль"/>
        </StackPanel>
        <StackPanel Margin="15" Orientation="Horizontal">
            <TextBox Name="BoxType" MinWidth="120"/>
            <Label Content="Тип"/>
        </StackPanel>
        <StackPanel Margin="15" Orientation="Horizontal">
            <TextBox Name="BoxTelefon" MinWidth="120"/>
            <Label Content="Телефон"/>
        </StackPanel>
        <StackPanel Margin="15" Orientation="Horizontal" HorizontalAlignment="Center">
            <Button Margin="0 0 15 0" Content="зарегистрироваться" Click="Reg_Click"/>
            <Button Content="отмена" Click="Close_Click"/>
        </StackPanel>
    </StackPanel>
</Window>
